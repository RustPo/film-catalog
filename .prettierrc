{
  "arrowParens": "always",
  "bracketSameLine": false,
  "bracketSpacing": true,
  "overrides": [
    {
      "files": ["*.html", "*.css", "*.scss"],
      "options": {
        "singleQuote": false,
        "htmlWhitespaceSensitivity": "ignore"
      }
    }
  ],
  "printWidth": 80,
  "semi": true,
  "singleQuote": true,
  "tabWidth": 2,
  "trailingComma": "es5"
}
